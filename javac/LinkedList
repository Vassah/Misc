//Linked List
public class LinkedList<E> {
	private class Node<E> {
		private E data;
		private Node<E> next;
		public Node(E content, Node<E> later) {
			this.data = content;
			this.next = next;
		}
		public Node(E content) {
			this.data = content;
			this.next = null;
		}
		//Setters
		public setNext(E stuff) {
			this.next = Node(E);
		}
		public insertBetween(E stuff) {
			this.next = Node(E, this.next);
		}
		//Getters
		public Node<E> getNext() {
			return this.next;
		}
		public Node<E> getContent() {
			return this.data;
		}
	}

	public LinkedList() {
	}

	public static void main(String[] args) {
		Node<Integer> thing = new Node<Integer>(5);
		System.out.format("%d", thing.getContent());
	}
}